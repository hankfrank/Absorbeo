TITLE=The Backdoor Factory
PREINSTALL=None
CLIENT_CMD=git clone
URL=https://github.com/secretsquirrel/the-backdoor-factory
PATH=/opt/the-backdoor-factory
INSTALL=cd /opt/the-backdoor-factory && ./install.sh && cd /opt/arsenal
